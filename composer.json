{
    "name": "hamidsamak/dropbox-api",
    "description": "A minimal implementation of Dropbox API v2 with syncing",
    "keywords": [
        "hamidsamak",
        "dropbox-api",
        "dropbox",
        "api",
        "v2",
        "sync",
        "syncing"
    ],
    "homepage": "https://github.com/hamidsamak/dropbox-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Vanderbist",
            "email": "alex.vanderbist@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Hamid Samak",
            "email": "hamidreza.samak@gmail.com",
            "homepage": "https://hamidsamak.ir",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "graham-campbell/guzzle-factory": "^3.0|^4.0|^5.0",
        "guzzlehttp/guzzle": "^6.2|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Dropbox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Dropbox\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "guzzlehttp/psr7": "<1.7.0"
    }
}
